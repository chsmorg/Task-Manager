{
    "version": 2,
    "builds": [
      {
        "src": "Task-Manager/package.json",
        "use": "@vercel/node"
      },
      {
        "src": "backend/package.json",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "backend/server.js"
      }
    ]
  }
  